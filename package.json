{
  "name": "remix-electron-template",
  "description": "",
  "version": "0.0.0",
  "private": true,
  "main": "desktop/index.js",
  "sideEffects": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development remix dev --command \"nodemon .\" --manual",
    "build": "chmod +r node_modules/node-unar/lsar node_modules/node-unar/unar && remix build && electron-builder",
    "start": "cross-env NODE_ENV=production electron .",
    "buildWin": "npm run installAllBinaries --prefix ./node_modules/node-7z-archive && chmod +r node_modules/node-unar/lsar node_modules/node-unar/unar && remix build && electron-builder --win --x64"
  },
  "//": "isexe (and which - necessary?) included for fluent ffmpeg though note which is in its asar.unpacked?",
  "build": {
    "files": [
      "build",
      "desktop",
      "public",
      "remix.config.js"
    ],
    "extraResources": [
      {
        "from": "data",
        "to": "data"
      },
      {
        "from": "dats",
        "to": "dats"
      },
      {
        "from": "assets",
        "to": "assets"
      }
    ],
    "asarUnpack": [
      "node_modules/fluent-ffmpeg/**/*",
      "node_modules/ffmpeg-static/**/*",
      "node_modules/isexe/**/*",
      "node_modules/which/**/*",
      "node_modules/node-7z-archive/node_modules/**/*",
      "node_modules/when/**/*",
      "node_modules/fs-extra/**/*",
      "node_modules/minimist/**/*",
      "node_modules/node-sys/**/*",
      "node_modules/node-unar/**/*",
      "node_modules/node-wget-fetch/**/*",
      "node_modules/universalify/**/*",
      "node_modules/graceful-fs/**/*",
      "node_modules/jsonfile/**/*"
    ]
  },
  "dependencies": {
    "@pdfslick/react": "^1.5.1",
    "@remix-run/css-bundle": "^2.1.0",
    "@remix-run/node": "^2.1.0",
    "@remix-run/react": "^2.1.0",
    "@szhsin/react-menu": "^4.1.0",
    "ag-grid-community": "^30.2.0",
    "ag-grid-react": "^30.2.0",
    "clsx": "^2.1.1",
    "electron-log": "^5.3.0",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "html-react-parser": "^5.1.10",
    "iconv-lite": "0.6.3",
    "ini": "^4.1.1",
    "isbot": "^3.7.0",
    "mime-types": "^2.1.35",
    "node-7z-archive": "github:tonywoode/node-7z-archive",
    "react": "^18.2.0",
    "react-arborist": "^3.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.4.0",
    "react-modal": "^3.16.1",
    "react-resizable": "^3.0.5",
    "react-split": "^2.0.14",
    "react-tabs": "^6.0.2",
    "remix-electron": "^2.0.0",
    "remix-utils": "^7.7.0",
    "tmp": "^0.2.3",
    "word-extractor": "^1.0.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.1.0",
    "@remix-run/eslint-config": "^2.1.0",
    "@remix-run/serve": "^2.1.0",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@types/react-modal": "^3.16.3",
    "@types/react-resizable": "^3.0.6",
    "cross-env": "^7.0.3",
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  }
}